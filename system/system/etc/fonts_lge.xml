<?xml version="1.0" encoding="utf-8"?>
<!--
    Added System Default Fonts Family.
-->
<!-- Android P familyset version update -->
<familyset version="23">

    <family name="default_lgsmartui">
        <font weight="400" style="normal">Roboto-Regular.ttf</font>
        <font weight="400" style="italic">Roboto-Italic.ttf</font>
        <font weight="700" style="normal">Roboto-Bold.ttf</font>
        <font weight="700" style="italic">Roboto-BoldItalic.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch START -->
        <font weight="100" style="normal">Roboto-Thin.ttf</font>
        <font weight="100" style="italic">Roboto-ThinItalic.ttf</font>
        <font weight="300" style="normal">Roboto-Light.ttf</font>
        <font weight="300" style="italic">Roboto-LightItalic.ttf</font>
        <font weight="500" style="normal">Roboto-Medium.ttf</font>
        <font weight="500" style="italic">Roboto-MediumItalic.ttf</font>
        <font weight="900" style="normal">Roboto-Black.ttf</font>
        <font weight="900" style="italic">Roboto-BlackItalic.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch END -->
    </family>

    <family name="lgsmartui">
        <font weight="400" style="normal">LG_Smart_UI_Mobile-Regular.ttf</font>
        <font weight="700" style="normal">LG_Smart_UI_Mobile-Bold.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch START -->
        <font weight="300" style="normal">LG_Smart_UI_Mobile-Light.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch END -->
    </family>
    <family name="lgkindgothic">
        <font weight="400" style="normal">HYKindGothic_Regular.ttf</font>
        <font weight="700" style="normal">HYKindGothic_Bold.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch START -->
        <font weight="300" style="normal">HYKindGothic_Light.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch END -->
    </family>
    <family name="lgfriend">
        <font weight="400" style="normal">HYFriend_Regular.ttf</font>
        <font weight="700" style="normal">HYFriend_Bold.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch START -->
        <font weight="300" style="normal">HYFriend_Light.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch END -->
    </family>
    <family name="lgtravel">
        <font weight="400" style="normal">HYTravel_Regular.ttf</font>
        <font weight="700" style="normal">HYTravel_Bold.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch START -->
        <font weight="300" style="normal">HYTravel_Light.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch END -->
    </family>
    <family name="lgyoyo">
        <font weight="400" style="normal">HYYoyo_Regular.ttf</font>
        <font weight="700" style="normal">HYYoyo_Bold.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch START -->
        <font weight="300" style="normal">HYYoyo_Light.ttf</font>
        <!-- 2018.09.10 Android P Weight Patch END -->
    </family>

    <alias name="default_lgsmartui-thin" to="lg-sans-serif" weight="100" />
    <alias name="default_lgsmartui-light" to="lg-sans-serif" weight="300" />
    <alias name="default_lgsmartui-medium" to="lg-sans-serif" weight="500" />
    <alias name="default_lgsmartui-black" to="lg-sans-serif" weight="900" />

    <alias name="lgsmartui-light" to="lg-lgsmartui" weight="300" />
    <alias name="lgkindgothic-light" to="lg-lgkindgothic" weight="300" />
    <alias name="lgfriend-light" to="lg-lgfriend" weight="300" />
    <alias name="lgtravel-light" to="lg-lgtravel" weight="300" />
    <alias name="lgyoyo-light" to="lg-lgyoyo" weight="300" />
</familyset>
